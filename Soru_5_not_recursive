def asal_mi(a):
    if a <= 1:
        return False
    for i in range(2,int(a**(0.5))+1):
        if a%i == 0:
            return False
    return True
def sÃ¼per_asal_sayi(x):
    x = str(x)
    if asal_mi(int(x)) and asal_mi(int(x[:4])) and asal_mi(int(x[:3]))and asal_mi(int(x[:2]))and asal_mi(int(x[:1])):
        return True
    return False
for i in range(10000,100000):
    if sÃ¼per_asal_sayi(i):
        print(i)
